      "type": "string"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string"
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string"
    },
    "address": {
      "$id": "#/properties/address",
      "type": "string"
    },
    "lat": {
      "$id": "#/properties/lat",
      "type": "number"
    },
    "lng": {
      "$id": "#/properties/lng",
      "type": "number"
    }
